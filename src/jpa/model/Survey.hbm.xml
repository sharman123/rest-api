<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="jpa.model">
    <class name="Survey" table="SURVEY">
        <id name="surveyId" column="SURVEY_ID">
            <generator class="native"/>
        </id>
        <property name="fName" column="fName" />
		<property name="lName" column="lName" />
		<property name="streetAddress" column="streetAddress" />
		<property name="city" column="city" />
		<property name="state" column="state" />
		<property name="phonenumber" column="phonenumber" />
		<property name="email" column="email" />
		<property name="howYouHeard" column="howYouHeard" />
		<property name="recommendation" column="recommendation" />
		<property name="rafflenumbers" column="rafflenumbers" />
		<property name="comments" column="comments" />
		<property name="zipcode" column="zipcode" />
		<property name="surveydate" column="surveydate" />
		<property name="likereasons" column="likereasons" />
 
    </class> 
</hibernate-mapping>